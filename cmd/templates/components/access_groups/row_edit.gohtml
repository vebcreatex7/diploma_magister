<tr id="access-groups-row-{{.UID}}">
    <td>
        <input
            type="text"
            data-include-edit="{{.UID}}"
            name="name"
            value="{{.Name}}">
    </td>
    <td>
        <input
            type="text"
            data-include-edit="{{.UID}}"
            name="description"
            value="{{.Description}}">
    </td>
    <td>
        <input
            type="text"
            data-include-edit="{{.UID}}"
            name="users"
            value="{{.Users}}">
    </td>
    <td>
        <input
                type="text"
                data-include-edit="{{.UID}}"
                name="equipment"
                value="{{.Equipment}}">
    </td>
    <td>
        <input
                type="text"
                data-include-edit="{{.UID}}"
                name="inventory"
                value="{{.Inventory}}">
    </td>
    <td>
        <a
                hx-put="/admin/access-groups/{{.UID}}"
                hx-target="#access-groups-row-{{.UID}}"
                hx-swap="outerHTML"
                hx-indicator="#processing"
                hx-include="input[data-include-edit='{{.UID}}'], select[data-include-edit='{{.UID}}']"
                hx-confirm="Подтвердите изменение группы'{{.Name}}'"
                href="">
            Изменить
        </a>
    </td>
    <td>
        <a
            hx-get="/admin/access-groups/{{.UID}}"
            hx-target="#access-groups-row-{{.UID}}"
            hx-swap="outerHTML"
            hx-indicator="#processing"
            href="">
            Отменить
        </a>
    </td>
</tr>
